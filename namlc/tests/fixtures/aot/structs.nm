struct Point {
    x: int,
    y: int
}

struct Rectangle {
    width: int,
    height: int
}

fn main() {
    var p: Point = Point { x: 5, y: 10 };
    if (p.x != 5) { panic("x"); }
    if (p.y != 10) { panic("y"); }

    p.x = 15;
    if (p.x != 15) { panic("set x"); }

    var rect: Rectangle = Rectangle { width: 100, height: 50 };
    var area: int = rect.width * rect.height;
    if (area != 5000) { panic("area"); }

    println("OK");
}
