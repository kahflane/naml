fn main() {
    var x: int = 10;
    if (x > 5) {
        x = x + 1;
    } else {
        panic("if");
    }
    if (x != 11) { panic("if result"); }

    var sum: int = 0;
    var i: int = 0;
    while (i < 10) {
        sum = sum + i;
        i = i + 1;
    }
    if (sum != 45) { panic("while"); }

    var range_sum: int = 0;
    for (j: int in 0..5) {
        range_sum = range_sum + j;
    }
    if (range_sum != 10) { panic("for range"); }

    var day: int = 3;
    var matched: int = 0;
    switch (day) {
        case 1: { matched = 1; }
        case 3: { matched = 3; }
        default: { matched = -1; }
    }
    if (matched != 3) { panic("switch"); }

    println("OK");
}
