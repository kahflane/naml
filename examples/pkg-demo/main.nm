use mathlib::*;

fn main() {
    var result: Math = add(10, 20);
    println(fmt("10 + 20 = {}", result));

    var product: Math = multiply(6, 7);
    println(fmt("6 * 7 = {}", product));

    var factorial: Math = factorial(5);
    println(fmt("5! = {}", factorial));

    var m: Mathematician = Mathematician{i: 1};
}
