use std::testing::*;

fn d(f: fn(int) -> int, y: int) -> int {
    return f(y);
}

fn main() {
    var x: int = 1;
    var y: int = 2;
    var z: fn(int)->int = fn(x: int) -> int { return x + 1; };
    var result: int = d(z, y);
    println(result);
    assert_eq(result, 3, "d(z, y) should return 3 (2 + 1)");
}
